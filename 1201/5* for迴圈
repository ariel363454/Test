#include <stdio.h>
int main()
{
  int x, y;
  for(x=1; x<=5; x+=2){
    for(y=0; y<=5-x; y++){
      printf(" ");
    }
    for(y=0; y<=(5-x)/2; y++){
      printf("*");
    }
    printf("\n");
  }
  for(x=3; x>0; x-=2){
    for(y=0; y<=5-x; y++){
      printf(" ");
    }
    for(y=0; y<=(5-x)/2; y++){
      printf("*");
    }
    printf("\n");
  }
}
